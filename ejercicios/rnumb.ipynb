    "import random\n",
    "\n",
    "numlist = []\n",
    "number = random.randint(1, 8)\n",
    "numlist.append(number)\n",
    "\n",
    "\n",
    "while len(numlist)<=7:\n",
    "    number = random.randint(1, 8)\n",
    "    while number not in numlist:\n",
    "        numlist.append(number)\n",
    "print(numlist)"
